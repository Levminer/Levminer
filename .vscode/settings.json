{
	"editor.autoClosingBrackets": "always",
	"files.autoSave": "afterDelay",
	"explorer.confirmDelete": false,
	"git.ignoreMissingGitWarning": true,
	"liveServer.settings.donotShowInfoMsg": true,
	"code-runner.runInTerminal": true,
	"workbench.iconTheme": "material-icon-theme",
	"material-icon-theme.folders.theme": "none",
	"prettier.useTabs": true,
	"html.validate.scripts": false,
	/* 	"typescript.validate.enable": false, */
	"[javascript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},

	"better-comments.tags": [
		{
			"tag": "!",
			"color": "red",
			"strikethrough": false,
			"backgroundColor": "transparent"
		},
		{
			"tag": "?",
			"color": "orange",
			"strikethrough": false,
			"backgroundColor": "transparent"
		},
		{
			"tag": "//",
			"color": "green",
			"strikethrough": true,
			"backgroundColor": "transparent"
		}
	],
	"[html]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"prettier.printWidth": 500,
	"[typescript]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	/* "js/ts.implicitProjectConfig.checkJs": true, */
	"editor.snippetSuggestions": "inline",
	"editor.suggestSelection": "first",
	"explorer.confirmDragAndDrop": false,
	"editor.formatOnSave": true,
	"editor.suggest.snippetsPreventQuickSuggestions": false,
	"editor.wordBasedSuggestionsMode": "allDocuments",
	"editor.linkedEditing": true,
	"omnisharp.useEditorFormattingSettings": false,
	"python.languageServer": "Microsoft",
	"update.showReleaseNotes": false,
	"settingsSync.keybindingsPerPlatform": false,
	"workbench.activityBar.visible": true,
	"prettier.endOfLine": "crlf",
	"prettier.tabWidth": 4,
	"eslint.format.enable": true,
	"material-icon-theme.files.associations": {
		".webpackrc.js": "webpack",
		"router.js": "javascript",
		".tailwindrc.js": "tailwindcss",
		".snowpackrc.js": "snowpack"
	},
	"editor.codeActionsOnSave": {
		"source.fixAll.eslint": false
	},
	"eslint.validate": ["javascript"],
	"files.associations": {
		"*.ejs": "html",
		"*.authme": "json",
		"*.authmeimport": "json",
		"*.authmebackup": "json"
	},
	"prettier.semi": false,
	"[jsonc]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[json]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[vue]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"workbench.startupEditor": "newUntitledFile",
	"[javascriptreact]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"csharp.format.enable": false,
	"liveSassCompile.settings.formats": [
		{
			"format": "compressed"
		}
	],
	"liveSassCompile.settings.generateMap": false,
	"editor.multiCursorModifier": "alt",
	"javascript.updateImportsOnFileMove.enabled": "always",
	"tabnine.experimentalAutoImports": true,
	"files.autoSaveDelay": 100,
	"css.customData": ["./css_custom_data.json"],
	"css.lint.unknownAtRules": "ignore",
	"scss.lint.unknownAtRules": "ignore",
	"redhat.telemetry.enabled": false,
	"editor.inlineSuggest.enabled": true,
	"gitlens.advanced.messages": {
		"suppressGitMissingWarning": true
	},
	"liveServer.settings.donotVerifyTags": true,
	"editor.bracketPairColorization.enabled": true,
	"bracketLens.mode": "auto",
	"cSpell.userWords": ["APPDATA", "Authme", "buttonmselected", "buttonr", "ipcs", "Levente", "levminer", "LÅ‘rik", "noopener", "noreferrer", "nsis", "otpauth", "qrcode", "qrcodedecoder", "readed", "submenu", "sveltejs", "tabcontent", "tabindex", "tablinks", "tauri", "titlebar", "totp", "unhash"],
	"workbench.editor.untitled.hint": "hidden",
	"editor.guides.bracketPairs": true,
	"svelte.ask-to-enable-ts-plugin": false,
	"editor.fontFamily": "Cascadia Light, Consolas, 'Courier New', monospace",
	"editor.fontSize": 15,
	"terminal.integrated.fontSize": 15,
	"workbench.colorTheme": "#000000",
	"gitlens.codeLens.enabled": false,
	"gitlens.plusFeatures.enabled": false,
	"stretchReminder.intervalBetweenStretch": 20,
	"[scss]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"[css]": {
		"editor.defaultFormatter": "esbenp.prettier-vscode"
	},
	"turboConsoleLog.includeFileNameAndLineNum": false,
	"github.copilot.enable": {
		"*": true,
		"yaml": false,
		"plaintext": false,
		"markdown": false,
		"javascript": true
	},
	"gitlens.hovers.currentLine.over": "line",
	"explorer.fileNesting.enabled": true,
	"gitlens.hovers.avatars": false,
	"gitlens.hovers.pullRequests.enabled": false,
	"gitlens.hovers.currentLine.details": false,
	"gitlens.hovers.annotations.details": false,
	"gitlens.hovers.enabled": false,
	"explorer.fileNesting.expand": false
}
